package com.cabletech.common.base;

import java.io.Serializable;
/**
 * 基类实体
 * @author wangt
 *
 */
public class BaseEntity implements Serializable {
	private static final long serialVersionUID = 1L;
	/**
	 * 主键ID
	 */
	private String id;

	/**
	 * 获取ID
	 * 
	 * @return
	 */
	public String getId() {
		return id;
	}

	/**
	 * 设置ID
	 * 
	 * @param id 
	 */
	public void setId(String id) {
		this.id = id;
	}
}
