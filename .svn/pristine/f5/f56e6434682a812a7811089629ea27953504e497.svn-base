<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@include file="/common/taglibs.jsp"%>
<table border="0" align="center" cellpadding="3" cellspacing="0">
	<tr>
		<th style="width: 13%">
			主题：
		</th>
		<td colspan="5" style="width: 87%">
			${wtroubleEoms.title }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			网络分类：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.networktype }
		</td>
		<th style="width: 13%">
			是否影响业务：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.isaffectbusiness }
		</td>
		<th style="width: 13%">
			影响业务类型：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.affectbusinesstype }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			故障发生时间：
		</th>
		<td style="width: 20%">
			<fmt:formatDate value="${wtroubleEoms.happentime }" pattern="yyyy-MM-dd HH:mm:ss" />
		</td>
		<th style="width: 13%">
			故障响应级别：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.responselevel }
		</td>
		<th style="width: 13%">
			告警级别：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.alarmlevel }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			受理时限：
		</th>
		<td style="width: 20%">
			<fmt:formatDate value="${wtroubleEoms.acceptedtime }" pattern="yyyy-MM-dd HH:mm:ss" />
		</td>
		<th style="width: 13%">
			处理时限：
		</th>
		<td style="width: 20%">
			<fmt:formatDate value="${wtroubleEoms.processtime }" pattern="yyyy-MM-dd HH:mm:ss" />
		</td>
		<th style="width: 13%">
			故障发现方式：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.findtype }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			T2处理时限：
		</th>
		<td style="width: 20%">
			<fmt:formatDate value="${wtroubleEoms.processtimet2 }" pattern="yyyy-MM-dd HH:mm:ss" />
		</td>
		<th style="width: 13%">
			网元名称：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.networkelementname }
		</td>
		<th style="width: 13%">
			故障设备厂商：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.equipmentcompany }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			故障设备类型：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.equipmenttype }
		</td>
		<th style="width: 13%">
			故障设备型号：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.equipmentmodel }
		</td>
		<th style="width: 13%">
			告警流水号：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.alarmserial }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			告警状态：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.alarmstatus }
		</td>
		<th style="width: 13%">
			告警逻辑分类：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.alarmlogictype }
		</td>
		<th style="width: 13%">
			告警逻辑子类：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.alarmlogicsubtype }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			告警来源：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.alarmsource }
		</td>
		<th style="width: 13%">
			相关投诉工单号：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.complaintsorder }
		</td>
		<th style="width: 13%">
			原始告警号：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.alarmoldcode }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			派单联系人：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.sendcontact }
		</td>
		<th style="width: 13%">
			EOMS工单编号：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.eomsid }
		</td>
		<th style="width: 13%">
			最终网络分类：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.incendnettype }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			最终设备厂商：
		</th>
		<td style="width: 20%">
			${wtroubleEoms.incendequipment }
		</td>
		<th style="width: 13%">
			是否重大故障：
		</th>
		<td style="width: 54%" colspan="3">
			${wtroubleEoms.incisimportantincident }
		</td>
	</tr>
	<tr>
		<th style="width: 13%">
			告警描述：
		</th>
		<td colspan="5" style="width: 87%">
			${wtroubleEoms.alarmdesc }
		</td>
	</tr>
</table>