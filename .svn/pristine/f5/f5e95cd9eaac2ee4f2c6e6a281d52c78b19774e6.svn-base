<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cabletech.business.wplan.plan.model.PatrolApprove"  table="WPLAN_PATROLAPPROVE">
        <!-- A 32 hex character is our surrogate key. It's automatically
            generated by Hibernate with the UUID pattern. -->
       	<id name="id" type="string" column="id">
			<generator
				class="com.cabletech.common.util.StrSequenceGenerator">
				<param name="sequence">SEQ_WPLAN_ID</param>
				<param name="length">12</param>
			</generator>
		</id>
		<property name="planid" >
		 <column name="PLAN_ID" />
		</property>
		<property name="approver" >
		 <column name="APPROVER" />
		</property>
		<property name="result" >
		 <column name="RESULT" />
		</property>
		<property name="attitude" >
		 <column name="ATTITUDE" />
		</property>
		<property name="approvetime" type="java.util.Date" >
		 <column name="APPROVE_TIME" />
		</property>
    </class>
</hibernate-mapping>
