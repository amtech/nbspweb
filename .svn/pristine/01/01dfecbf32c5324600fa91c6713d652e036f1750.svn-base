<?xml version="1.0" encoding="UTF-8"?>
<root id="simpleInputElements">
	<input-element id="simple-common">
		<![CDATA[
		<td class='tdulright' style='width:20%'>
		<input id='planPatrolResult.subItemIds' 
		 name='planPatrolResult.subItemIds' value='#itemId' type='hidden' />
		#itemName
		</td>
		<td class='tdulright' style='width:40%'>
		#itemInputElement
		</td>
		<td class='tdulright' style='width:40%'>
		#itemTextArea
		</td>
		]]>
	</input-element>
	<input-element id="textarea">
		<![CDATA[
		<input id='planPatrolResult.exceptionDescs' name='planPatrolResult.exceptionDescs' 
		 value='' type='text' class='inputtext' />
		</textarea> 
		]]>
	</input-element>
	<input-element id="number">
		<![CDATA[
		<input id='subItemPatrols_#itemId' name='subItemPatrols_#itemId' 
		 value='' type='text' class='inputtext' />
		]]>
	</input-element>
	<input-element id="text">
		<![CDATA[
		<input id='subItemPatrols_#itemId' name='subItemPatrols_#itemId' 
		 value='' type='text' class='inputtext' /> 
		]]>
	</input-element>
	<input-element id="choose">
		<![CDATA[
		<input id='subItemPatrols_#itemId' name='subItemPatrols_#itemId' 
		 value='#itemValue' type='radio' #itemChecked /> 
		#itemValue
		]]>
	</input-element>
</root>