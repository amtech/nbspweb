package com.cabletech.common.excel.parameter;

import org.apache.poi.hssf.usermodel.HSSFCellStyle;

/**
 * 对齐参数
 * @author wangt
 *
 */
public class AlignmentParameter {
	private short vAlignment;
	private short alignment;

	/**
	 * 构造方法初始化属性
	 */
	public AlignmentParameter() {
		vAlignment = HSSFCellStyle.VERTICAL_CENTER;
		alignment = HSSFCellStyle.ALIGN_CENTER;
	}

	/**
	 * 
	 * @return
	 */
	public short getvAlignment() {
		return vAlignment;
	}

	/**
	 * 
	 * @param vAlignment short
	 */
	public void setvAlignment(short vAlignment) {
		this.vAlignment = vAlignment;
	}

	public short getAlignment() {
		return alignment;
	}

	public void setAlignment(short alignment) {
		this.alignment = alignment;
	}
}
