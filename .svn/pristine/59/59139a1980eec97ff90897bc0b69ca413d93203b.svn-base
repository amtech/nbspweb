<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cabletech.business.workflow.wmaintain.model.WMaintainSite"
		table="WMAINTAIN_SITE">
		<id name="id" type="string">
			<column name="ID" length="12" />
			<generator class="com.cabletech.common.util.StrSequenceGenerator">
				<param name="sequence">SEQ_WMAINTAIN_SITE_ID</param>
				<param name="length">12</param>
			</generator>
		</id>
		<property name="planId" type="string">
			<column name="PLAN_ID" />
		</property>
		<property name="rsId" type="string">
			<column name="RS_ID" />
		</property>
		<property name="rsType" type="string">
			<column name="RS_TYPE" />
		</property>
	</class>
</hibernate-mapping>
