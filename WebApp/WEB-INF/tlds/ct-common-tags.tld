<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>com.cabletech.common.tag</short-name>
	<uri>cabletechtag</uri>
	<description>用于系统公共模块的标签库</description>
	<tag>
		<name>upload</name>
		<tag-class>com.cabletech.common.tag.upload.UploadFileTag</tag-class>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<description>上传按钮样式设置</description>
		</attribute>
		<attribute>
			<name>state</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>state 状态包括 add，edit，look三种状态，默认为add</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>添加附件时保存在变量中的临时附件</description>
		</attribute>
		<attribute>
			<name>entityId</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>edit，look状态必须的，关联业务对象id</description>
		</attribute>
		<attribute>
			<name>entityType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>edit，look状态必须的，关联业务对象</description>
		</attribute>
		<attribute>
			<name>isOutScript</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>true:输出script脚本，false；不输出script脚本</description>
		</attribute>
		<attribute>
			<name>useable</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>状态是否可用</description>
		</attribute>
	</tag>
	<tag>
		<name>quickLoadList</name>
		<tag-class>com.cabletech.common.tag.QuickloadListsTag</tag-class>
		<attribute>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>常用列表类型select,radio,checkbox,view 默认为view</description>
		</attribute>
		<attribute>
			<name>columntype</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>数据字典的分类名称，详见数据库base_sysdictionary表</description>
		</attribute>
		<attribute>
			<name>keyValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>用于显示字典名称时，传入的字典对应的codevalue</description>
		</attribute>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>样式表</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>仅有select 时能够使用style</description>
		</attribute>
		<attribute>
			<name>isQuery</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<description>query 显示不限;select 显示请选择</description>
		</attribute>
		<attribute>
			<name>onChange</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<description>onChange 事件</description>
		</attribute>
		<attribute>
			<name>onClick</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<description>onClick 事件</description>
		</attribute>
	</tag>
</taglib>	