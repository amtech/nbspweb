<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cabletech.business.resource.model.RFIDAddress"
		table="RS_PATROLADDRESSINFO">
		<!-- A 32 hex character is our surrogate key. It's automatically
			generated by Hibernate with the UUID pattern. -->
		<id name="id" type="string" column="id">
			<generator
				class="com.cabletech.common.util.StrSequenceGenerator">
				<param name="sequence">SEQ_CERTIFICATE_ID</param>
				<param name="length">10</param>
			</generator>
		</id>
		<property name="restype" column="res_type"/>
		<property name="resid" column="res_id"/>
		<property name="rfid"/>
		<property name="address"/>
	</class>
</hibernate-mapping>