<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cabletech.business.ah.rating.model.RatingForm"
		table="AH_RATINGFORM">
		<id name="id" type="string" column="id">
			<generator class="com.cabletech.common.util.StrSequenceGenerator">
				<param name="sequence">SEQ_RATING_ID</param>
				<param name="length">12</param>
			</generator>
		</id>
		<property name="title" column="TITLE" />
		<property name="remark" column="REMARK" />
		<property name="useState" column="USE_STATE" />
		<property name="businessType" column="BUSINESS_TYPE" />
		<property name="creater" column="CREATER" />
		<property name="createTime" column="CREATE_TIME" />
	</class>
</hibernate-mapping>
